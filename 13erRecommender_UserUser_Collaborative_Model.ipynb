    "fname_14erList = r'14erChecklistByUser_df_winterfix'\n",
    "fname_13erList = r'13erChecklistByUser_df_winterfix'\n",
    "\n",
    "df_13ers = pickle.load(open(fname_13erList,\"rb\"))\n",
    "df_13ers = df_13ers[df_13ers['PeakName']!=\"\"]\n",
    "df_13ers = df_13ers.sort_values('PeakName')\n",
    "df_13ers['NumClimbs'] = df_13ers['NumClimbs'].apply(lambda x: -1 if x==0 else 1)\n",
    "\n",
    "fname_user = r'user_profile_data_FULL_winterfix'\n",
    "user_df = pickle.load(open(fname_user,\"rb\"))\n",
    "#user_df = user_df[['UserId','Username']]\n",
    "\n",
    "df_13ers = pd.merge(df_13ers, user_df[['UserId','Username']], on='UserId', how='left')\n",
    "df_13ers = df_13ers.drop_duplicates()\n",
    "df_13ers.head(10)\n",
    "#user_df.head()"
   ]
